---
import Head from "@components/Head.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <Head title={title} />
  <body class="bg-dark-blue">
    <div>
      <main>
        <slot />
      </main>
      <footer></footer>
    </div>
  </body>
</html>
